{
  "name": "jupiter-bot",
  "version": "1.0.0",
  "description": "Jupiter DEX aggregator trading bot for Solana",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "concurrently -k \"nodemon --exec ts-node src/server/dashboard.ts\" \"npx http-server public -p 3004 -c-1\"",
    "quote": "ts-node src/examples/getQuote.ts",
    "quote:mock": "ts-node src/examples/mockQuote.ts",
    "paper:demo": "ts-node src/test/paperTrading.ts",
    "paper:strategy": "ts-node src/examples/simpleStrategy.ts",
    "x402:demo": "ts-node src/examples/x402Demo.ts",
    "quote:raydium": "ts-node src/examples/getRaydiumQuote.ts",
    "debug:raydium": "ts-node src/examples/debugRaydium.ts",
    "start:dashboard": "ts-node src/server/dashboard.ts",
    "dashboard:ui": "npx http-server public -p 3004 -c-1",
    "start:all": "concurrently \"npm run start:api\" \"npm run start:ui\"",
    "start:api": "ts-node src/server/dashboard.ts",
    "start:ui": "npx http-server public -p 3004 -c-1 -o",
    "test": "jest",
    "test:indicators": "jest technicalIndicators.test.ts",
    "live": "ts-node src/examples/runLiveAnalysis.ts",
    "live:fast": "ts-node src/examples/runLiveAnalysis.ts",
    "wallet": "ts-node src/examples/checkWallet.ts",
    "auto:dry": "ts-node src/examples/startAutoTrader.ts",
    "view": "ts-node src/examples/viewOnlyAnalyzer.ts",
    "start:trading": "ts-node src/server/tradingBotAPI.ts",
    "start:x402": "ts-node src/server/x402Server.ts",
    "start:midnight": "ts-node src/server/midnightAPI.ts",
    "start:project": "ts-node src/server/projectDashboard.ts",
    "start:everything": "concurrently -n \"TRADING,X402,MIDNIGHT,PROJECT,DASHBOARD,UI,ANALYZER\" -c \"blue,green,magenta,cyan,yellow,white,gray\" \"npm run start:trading\" \"npm run start:x402\" \"npm run start:midnight\" \"npm run start:project\" \"npm run start:dashboard\" \"npm run dashboard:ui\" \"npm run view\"",
    "start:tracker": "ts-node src/server/projectTracker.ts",
    "start:all-with-tracker": "concurrently -n \"TRADING,X402,MIDNIGHT,PROJECT,DASHBOARD,TRACKER,UI\" -c \"blue,green,magenta,cyan,yellow,red,white\" \"npm run start:trading\" \"npm run start:x402\" \"npm run start:midnight\" \"npm run start:project\" \"npm run start:dashboard\" \"npm run start:tracker\" \"npm run dashboard:ui\""
  },
  "keywords": [
    "solana",
    "jupiter",
    "dex",
    "trading",
    "bot"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.13.1",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.4",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
